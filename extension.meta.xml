        <type>Other</type>
    </types>
    <authors>
        <author>
            <name github="kanduvisla" symphony="kanduvisla">Giel Berkers</name>
            <email>info@gielberkers.com</email>
            <website>http://www.gielberkers.com</website>
        </author>
    </authors>
    <releases>
        <release version="1.2" date="2011-12-16" min="2.3"/>
        <release version="1.1" date="2010-09-21" min="2.2.x"/>
